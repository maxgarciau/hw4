<h1>Places</h1>

<div class="mt-2">
<button type="button" class="btn btn-light"><a href="/places/new">New place</a>
  </button>
</div>

<ul style="padding: 0; list-style: none;">
  <% for place in @places %>
    <div class="mt-2">
    <li><h5><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></h5></li>
    </div>
  <% end %>
</ul>